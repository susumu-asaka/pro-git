git branch <branchname>
git branch -d <branchname>
git branch [-v[v]] [--merged|--no-merged]
git branch -u <upstream>
git checkout <branch>
git checkout --track <remote>/<branch>
git merge <commit>
git fetch --all
git rebase [-i] [--onto <newbase>] <upstream> [<branch>]
git rebase (--continue | --abort)